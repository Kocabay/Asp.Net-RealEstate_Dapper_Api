@using RealEstate_Dapper_UI.Dtos.PropertyImageDtos
@model List<PropertyImageDtos>

<div class="single-post-image mb-5">
    <div class="owl-blog owl-carousel owl-theme">
        @foreach (var item in Model)
        {
        <div class="item">
            <div class="card">
                <img src="@item.ImageUrl" style="height:550px" class="img-fluid radius-image" alt="image">
            </div>
        </div>
        
            
        }
    </div>
</div>